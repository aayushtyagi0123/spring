<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean class="com.springcore.springcore.Student" name="student1" p:studentId="12345" p:studentName="Aayush"
p:studentAddress="sre"/>

<bean class="com.springcore.springcore.Student" name="student2">
<property name="studentId" value="56789"/>
<property name="studentName" value="Mohit"/>
<property name="studentAddress" value="delhi"/>
</bean>
</beans>